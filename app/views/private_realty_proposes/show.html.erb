<%- model_class = PrivateRealtyPropose -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:District) %>:</strong></dt>
  <dd><%= @private_realty_propose.District %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Street) %>:</strong></dt>
  <dd><%= @private_realty_propose.Street %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Price) %>:</strong></dt>
  <dd><%= @private_realty_propose.Price %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Count_of_rooms) %>:</strong></dt>
  <dd><%= @private_realty_propose.Count_of_rooms %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Condition) %>:</strong></dt>
  <dd><%= @private_realty_propose.Condition %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Square) %>:</strong></dt>
  <dd><%= @private_realty_propose.Square %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Stage) %>:</strong></dt>
  <dd><%= @private_realty_propose.Stage %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Type) %>:</strong></dt>
  <dd><%= @private_realty_propose.Type %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Heating) %>:</strong></dt>
  <dd><%= @private_realty_propose.Heating %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Contacts) %>:</strong></dt>
  <dd><%= @private_realty_propose.Contacts %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Owner) %>:</strong></dt>
  <dd><%= @private_realty_propose.Owner %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Other) %>:</strong></dt>
  <dd><%= @private_realty_propose.Other %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              private_realty_proposes_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_private_realty_propose_path(@private_realty_propose), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              private_realty_propose_path(@private_realty_propose),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
